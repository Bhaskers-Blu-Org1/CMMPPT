wit-test-case pen-exec/case11 heur-spl3 $1
wit-test-case pen-exec/case12 heur-spl3 $1
wit-test-case pen-exec/case13 heur-spl3 $1
wit-test-case pen-exec/case14 heur-spl3 $1
wit-test-case pen-exec/case15 heur-spl3 $1
wit-test-case pen-exec/case16 heur-spl3 $1
wit-test-case pen-exec/case17 heur-spl3 $1
wit-test-case pen-exec/case18 heur-spl3 $1
wit-test-case pen-exec/case19 heur-spl3 $1
wit-test-case pen-exec/case20 heur-spl3 $1
wit-test-case pen-exec/case21 heur-spl3 $1
wit-test-case pen-exec/case22 heur-spl3 $1
wit-test-case pen-exec/case23 heur-spl3 $1
wit-test-case pen-exec/case24 heur-spl3 $1
