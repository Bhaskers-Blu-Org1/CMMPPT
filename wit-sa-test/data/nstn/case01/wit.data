//----------------------------
// NSTN Case 1
// Build-Ahead Upper Bounds
//----------------------------

release "5.0";

set problem
   nPeriods 4;

add part      "A" capacity;
add operation "B";
add bomEntry  "B" "A";
add part      "C" material;
add bopEntry  "B" "C";
add demand    "C" "D";

set part "A"
   supplyVol dense (10 10 10 10);

set demand "C" "D"
   demandVol dense (0 0 0 100);

set part "C"
   buildNstn    true
   buildAheadUB single (2);

