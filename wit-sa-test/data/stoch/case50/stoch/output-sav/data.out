set problem
nPeriods 3;

set problem;

add part "A" material;

add part "B" material;

add part "C" capacity;

add operation "D";

add bomEntry "D" "A";

add subEntry "D" 0 "B";

add bopEntry "D" "C";

add demand "C" "E";

//-----------------------------------------
// Undifferentiated Scenario-Specific Data
//-----------------------------------------

set operation "D"
execCost single (1);

set subEntry "D" 0 0
subCost single (1);

set demand "C" "E"
demandVol dense (100 0 0)
cumShipReward single (10);

//---------------------------
// Stochastic Implosion Data
//---------------------------

set problem
stageByObject true
nScenarios 2
stochMode true;

set part "A"
objectStage 1;

set part "B"
objectStage 1;

//-----------------------
// Data for Scenario #0
//-----------------------

set problem
currentScenario 0;

set part "A"
supplyVol dense (13 0 10);

set part "B"
supplyVol dense (5 10 0);

//-----------------------
// Data for Scenario #1
//-----------------------

set problem
currentScenario 1;

set part "A"
supplyVol dense (6 10 0);

set part "B"
supplyVol dense (14 0 10);

//----------------------------------
// Final Setting of CurrentScenario
//----------------------------------

set problem
currentScenario 1;

//----------------------------------
// End of Stochastic Implosion Data
//----------------------------------
