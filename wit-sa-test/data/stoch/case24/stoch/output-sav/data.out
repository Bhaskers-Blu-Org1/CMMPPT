set problem
nPeriods 1;

set problem;

add part "A" capacity;

add part "B" capacity;

add part "C" capacity;

add operation "D";

add operation "E";

add bomEntry "D" "A";

add bomEntry "D" "B";

add bomEntry "E" "B";

add bomEntry "E" "C";

//-----------------------------------------
