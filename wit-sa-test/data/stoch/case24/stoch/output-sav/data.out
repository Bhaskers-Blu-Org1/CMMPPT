// Data for Scenario #0
//-----------------------

set problem
currentScenario 0;

set part "A"
supplyVol single (300);

set part "B"
supplyVol single (300);

set part "C"
supplyVol single (200);

//-----------------------
// Data for Scenario #1
//-----------------------

set problem
currentScenario 1;

set part "A"
supplyVol single (300);

set part "B"
supplyVol single (300);

set part "C"
supplyVol single (100);

