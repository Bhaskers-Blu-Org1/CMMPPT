set problem
nPeriods 2
independentOffsets true;

set problem;

add part "A" capacity;

add part "B" capacity;

add part "C" material;

add operation "D";

add bomEntry "D" "A"
consRate single (3)
offset single (1);

add subEntry "D" 0 "B"
consRate single (4)
offset single (1);

add bopEntry "D" "C"
offset single (1)
productRate single (2);

add demand "C" "E";

//-----------------------------------------
// Undifferentiated Scenario-Specific Data
//-----------------------------------------

set part "A"
supplyVol dense (300 900)
scrapCost single (10);

set part "B"
supplyVol dense (400 800)
scrapCost single (10);

set part "C"
scrapCost single (10)
stockCost single (1);

set operation "D"
execCost single (100);

set subEntry "D" 0 0
subCost single (1);

set demand "C" "E"
demandVol single (600)
shipReward single (1000)
cumShipReward single (100);

//---------------------------
// Stochastic Implosion Data
//---------------------------

set problem
stageByObject true
nScenarios 2
stochMode true;

//-----------------------
// Data for Scenario #0
//-----------------------

set problem
currentScenario 0;

set problem
probability 0.4;

set demand "C" "E"
cumShipBounds
softLB single (200)
endBounds;

//-----------------------
// Data for Scenario #1
//-----------------------

set problem
currentScenario 1;

set problem
probability 0.6;

set demand "C" "E"
cumShipBounds
softLB single (300)
endBounds;

//----------------------------------
