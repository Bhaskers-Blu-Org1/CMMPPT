//---------------------------------------------
// Selection Recovery Case 2
//
// NSTN Build-Ahead
//---------------------------------------------

release "6.0";

set problem           nPeriods          3;
set problem           selectionRecovery true;

add part      "A"                       capacity;
add part      "B"                       material;
add part      "C"                       capacity;
add part      "D"                       capacity;

add operation "E";
add operation "F";
add operation "G";

add demand    "C" "H";
add demand    "D" "I";

add bomEntry  "E" "A";
add bomEntry  "F" "B";
add bomEntry  "G" "B";

add bopEntry  "E" "B";
add bopEntry  "F" "C";
add bopEntry  "G" "D";

set operation "F"     incLotSize        single (1000);

set part      "B"     buildNstn         true;

set part      "A"     supplyVol         dense  (1400 200 500);

set demand    "C" "H" priority          single (1);
set demand    "D" "I" priority          single (2);

set demand    "C" "H" demandVol         dense  (0 0 1000);
set demand    "D" "I" demandVol         dense  (0 0 1000);
