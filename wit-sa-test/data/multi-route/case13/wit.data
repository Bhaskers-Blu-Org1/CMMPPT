//-----------------------
// Non-exploding SubEntry
//-----------------------

release "5.0";

set problem
   multiRoute true
   nPeriods 1;

add part "A" material;
add part "C" material;
add part "D" material;
add part "E" material;
add part "G" material;

add operation "B";
add operation "F";

add demand "G" "H";

add bomEntry "B" "A";
add bomEntry "F" "C";

add bopEntry "B" "E";
add bopEntry "F" "G";

add subEntry "F" 0 "D";
add subEntry "F" 0 "E";

set part "A"
   supplyVol dense (10);

set part "C"
   supplyVol dense (10);

set part "D"
   supplyVol dense (10);

set part "E"
   supplyVol dense (10);

set demand "G" "H"
   demandVol dense (50);
