//-----------------
// Netting Order
// (no multi-route)
//-----------------

release "5.0";

set problem
   nPeriods 1;

add part "A" material;
add part "B" material;
add part "C" material;
add part "D" material;
add part "F" material;

add operation "E";

add demand "F" "G";

add bomEntry "E" "A";

add bopEntry "E" "F";

add subEntry "E" 0 "B";
add subEntry "E" 0 "C";
add subEntry "E" 0 "D";

set demand "F" "G"
   demandVol dense (16);

set part "B"
   supplyVol dense (10);

set part "C"
   supplyVol dense (10);

set part "D"
   supplyVol dense (10);

set subEntry "E" 0 0
   expNetAversion  1.0;

set subEntry "E" 0 1
   expNetAversion  8.0;

set subEntry "E" 0 2
   expNetAversion  0.0;
