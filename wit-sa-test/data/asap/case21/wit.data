//----------------------------------------------
// ASAP Test Problem Hand-Made From Scratch
//
// Case 21
// Mand ECs and buildAheadUB
// Same as case 11, but with buildAheadUB.
//----------------------------------------------

release "5.0";

set problem
   nPeriods 8;

add part      "A" material;
add part      "B" material;
add operation "C";
add bomEntry  "C" "A";
add bomEntry  "C" "B";
add part      "D" material;
add bopEntry  "C" "D";
add operation "E";
add bomEntry  "E" "A";
add bomEntry  "E" "D";
add part      "F" material;
add bopEntry  "E" "F";
add demand    "F" "G";

set part "D"
   buildAsap true
   buildAheadUB single (1);

set part "F"
   buildAsap true
   buildAheadUB single (1);

set bomEntry "C" 0
   latestPeriod   3
   mandEC         true;

set bomEntry "E" 0
   earliestPeriod 6
   mandEC         true;

set part     "A"
   supplyVol sparse (0:100);

set part     "B"
   supplyVol single (10);

set demand   "F" "G"
   demandVol dense (2 2 2 2 2 2 2 100);
