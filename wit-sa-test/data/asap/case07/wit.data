//-----------------------------------------
// ASAP Test Problem Hand-Made From Scratch
//
// Case 7
// Same as Case 2, but with hard upper
// bounds on execVol.
//-----------------------------------------

release "5.0";

set problem
   nPeriods 7;

add part      "A" material;
add operation "B";
add part      "C" material;
add operation "D";
add part      "E" material;
add demand    "E" "F";

add bomEntry  "B" "A";
add bopEntry  "B" "C";
add bomEntry  "D" "C";
add bopEntry  "D" "E";

set bopEntry "B" 0
   offset single (-2);

set bopEntry "D" 0
   offset single (-2);

set part "A"
   supplyVol sparse (1: 10);

set demand "E" "F"
   demandVol sparse (6: 14);

set part "A"
   buildAsap true;

set part "C"
   buildAsap true;

set part "E"
   buildAsap true;

set operation "B"
   execBounds
      hardUB single (3)
      endBounds;
