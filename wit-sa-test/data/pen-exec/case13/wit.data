//-----------------------------
// Penalized Execution Case 13:
// Substitute Netting
//-----------------------------

release "5.0";

set problem nPeriods   1;
set problem multiRoute true;
set problem penExec    true;

add part      "A" material;
add operation "B";
add bomEntry  "B" "A";
add part      "C" material;
add operation "D";
add bomEntry  "D" "C";
add part      "E" material;
add bopEntry  "D" "E";
add part      "F" material;
add operation "G";
add bomEntry  "G" "F";
add part      "H" material;
add bopEntry  "G" "H";
add part      "I" material;
add operation "J";
add bomEntry  "J" "E";
add bomEntry  "J" "H";
add subEntry  "J" 1 "I";
add part      "K" material;
add bopEntry  "B" "K";
add bopEntry  "J" "K";
add demand    "K" "L";

set part      "A"
   supplyVol dense (100);

set part      "C"
   supplyVol dense (100);

set part      "F"
   supplyVol dense (100);

set part      "I"
   supplyVol dense (10);

set demand    "K" "L"
   demandVol dense (20);

set operation "B"
   execPenalty 2;

set operation "D"
   execPenalty 1;

set operation "G"
   execPenalty 2;

set subEntry "J" 1 0
   expAllowed false;
