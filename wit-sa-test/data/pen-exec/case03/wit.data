//----------------------------------------
// Penalized Execution Case 3
// Case 2 with Large Demand.
//----------------------------------------

release "5.0";

set problem nPeriods   1;
set problem multiRoute true;
set problem penExec    true;

add part      "A" material;
add operation "B";
add bomEntry  "B" "A";
add part      "C" material;
add bopEntry  "B" "C";
add operation "D";
add bomEntry  "D" "C";
add part      "E" material;
add bopEntry  "D" "E";
add operation "F";
add bomEntry  "F" "E";

add part      "G" material;
add operation "H";
add bomEntry  "H" "G";
add part      "I" material;
add bopEntry  "H" "I";

add bopEntry  "F" "I";
add operation "J";
add bomEntry  "J" "I";
add bomEntry  "J" "C";
add part      "K" material;
add bopEntry  "J" "K";
add demand    "K" "L";

set operation "H"
   execPenalty 2;

set operation "F"
   execPenalty 1;

set operation "B"
   execPenalty 2;

set part      "G"
   supplyVol dense (100);

set part      "A"
   supplyVol dense (200);

set part      "C"
   supplyVol dense (7);

set part      "E"
   supplyVol dense (4);

set part      "I"
   supplyVol dense (5);

set part      "K"
   supplyVol dense (3);

set demand    "K" "L"
   demandVol dense (500);
