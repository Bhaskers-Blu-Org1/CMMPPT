//------------------------------------------------
// Penalized Execution Case 12:
// Case 7 with Substitutes instead of BOP Entries.
//------------------------------------------------

release "5.0";

set problem nPeriods   3;
set problem multiRoute true;
set problem penExec    true;

add part      "A" material;
add operation "B";
add bomEntry  "B" "A";
add part      "C" capacity;
add bopEntry  "B" "C";
add operation "D";
add bomEntry  "D" "C";
add operation "E";
add bomEntry  "E" "A";
add part      "F" capacity;
add bopEntry  "E" "F";
add operation "G";
add bomEntry  "G" "F";
add part      "J" capacity;
add bopEntry  "D" "J";
add part      "K" capacity;
add bopEntry  "G" "K";
add operation "L";
add bomEntry  "L" "K";
add subEntry  "L" 0 "J";
add part      "H" material;
add bopEntry  "L" "H";
add demand    "H" "I";

set part      "A"
   supplyVol single (10);

set part      "C"
   supplyVol single (10);

set part      "F"
   supplyVol single (10);

set part      "H"
   supplyVol single (10)
   buildNstn true;

set demand    "H" "I"
   demandVol sparse (2:1000);

set operation "B"
   execPenalty 2;

set operation "D"
   execPenalty 1;

set operation "E"
   execPenalty 2;

set operation "G"
   execPenalty 2;

set bomEntry "D" 0
   usageRate 2;

set bopEntry "E" 0
   prodRate 0.5;

set operation "B"
   execBounds
      hardUB single (3)
      endBounds;

set bomEntry "G" 0
   offset dense (0 1 1);

set bomEntry "B" 0
   latestPeriod 1;

set bopEntry "E" 0
   offset dense (-1 0 0);

set subEntry "L" 0 0
   netAllowed false;
