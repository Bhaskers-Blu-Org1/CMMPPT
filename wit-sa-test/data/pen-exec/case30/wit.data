//------------------------------------------
// Penalized Execution Case 30
// Same as Case 30, but with an execPenalty
// on BomEntry P-G
//------------------------------------------

release "6.0";

set problem               nPeriods          1;
set problem               multiRoute        true;
set problem               penExec           true;

add part      "A"         material;
add operation "B";
add bomEntry  "B" "A";
add part      "C"         material;
add operation "D";
add bomEntry  "D" "C";
add part      "E"         material;
add operation "F";
add bomEntry  "F" "E";
add part      "G"         material;
add bopEntry  "B" "G";
add bopEntry  "D" "G";
add bopEntry  "F" "G";
add part      "H"         material;
add operation "I";
add bomEntry  "I" "H";
add part      "J"         material;
add operation "K";
add bomEntry  "K" "J";
add part      "L"         material;
add operation "M";
add bomEntry  "M" "L";
add part      "N"         material;
add bopEntry  "I" "N";
add bopEntry  "K" "N";
add bopEntry  "M" "N";
add operation "P";
add bomEntry  "P" "G";
add subEntry  "P"  0  "N";
add part      "Q"         material;
add bopEntry  "P" "Q";
add demand    "Q" "R";

set subEntry  "P"  0   0  netAllowed        false;

set subEntry  "P"  0   0  execPenalty       2;
set bomEntry  "P"  0      execPenalty       4;

set operation "D"         execPenalty       3;
set operation "F"         execPenalty       4;
set operation "K"         execPenalty       3;
set operation "M"         execPenalty       4;

set part      "A"         supplyVol         dense (10);
set part      "C"         supplyVol         dense (10);
set part      "E"         supplyVol         dense (10);
set part      "H"         supplyVol         dense (10);
set part      "J"         supplyVol         dense (10);
set part      "L"         supplyVol         dense (10);

set demand    "Q" "R"     demandVol         dense (57);
