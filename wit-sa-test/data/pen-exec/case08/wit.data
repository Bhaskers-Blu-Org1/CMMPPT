//----------------------------------------
// Penalized Execution Case 8:
// Case 8 with a 0 usage rate.
//----------------------------------------

release "5.0";

set problem nPeriods   1;
set problem multiRoute true;
set problem penExec    true;

add part      "A" material;
add operation "B";
add bomEntry  "B" "A";
add part      "C" material;
add bopEntry  "B" "C";
add operation "D";
add bomEntry  "D" "C";
add operation "E";
add bomEntry  "E" "A";
add part      "F" material;
add bopEntry  "E" "F";
add operation "G";
add bomEntry  "G" "F";
add part      "H" material;
add bopEntry  "D" "H";
add bopEntry  "G" "H";
add demand    "H" "I";

set part      "A"
   supplyVol dense (10);

set part      "C"
   supplyVol dense (10);

set part      "F"
   supplyVol dense (10);

set part      "H"
   supplyVol dense (10);

set demand    "H" "I"
   demandVol dense (100);

set bopEntry  "D" 0
   expAversion 1;

set operation "B"
   execPenalty 2;

set operation "D"
   execPenalty 1;

set operation "E"
   execPenalty 2;

set operation "G"
   execPenalty 2;

//----------
// New Stuff
//----------

set bomEntry "D" 0
   usageRate -1;
