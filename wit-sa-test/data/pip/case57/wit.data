//---------------------------------------------------------------
// PIP Case 57
// Case 10, with PIP to Opns.
//---------------------------------------------------------------

release "6.0";

set problem               nPeriods       1;
set problem               pipSeqFromHeur true;

add part      "A"         capacity;
add part      "B"         capacity;
add operation "C";
add bomEntry  "C" "A";
add bomEntry  "C" "B";
add part      "D"         capacity;
add bopEntry  "C" "D";
add operation "E";
add bomEntry  "E" "D";
add part      "F"         capacity;
add bopEntry  "E" "F";
add demand    "F" "G";
add part      "H"         capacity;
add operation "I";
add bomEntry  "I" "B";
add bomEntry  "I" "H";
add part      "J"         capacity;
add bopEntry  "I" "J";
add operation "K";
add bomEntry  "K" "D";
add bomEntry  "K" "J";
add part      "L"         capacity;
add bopEntry  "K" "L";
add demand    "L" "M";

set demand    "F" "G"     priority       dense (1);
set demand    "L" "M"     priority       dense (2);

set bomEntry  "C"  1      consRate       dense (3);
set bomEntry  "I"  0      consRate       dense (2);
set bomEntry  "I"  1      consRate       dense (4);

set bopEntry  "I"  0      productRate    dense (0.5);

set part      "A"         supplyVol      dense (100);
set part      "B"         supplyVol      dense (100);
set part      "H"         supplyVol      dense (100);

set part      "D"         supplyVol      dense   (2);
set part      "F"         supplyVol      dense   (2);
set part      "J"         supplyVol      dense   (2);
set part      "L"         supplyVol      dense   (3);

set demand    "F" "G"     demandVol      dense  (10);
set demand    "L" "M"     demandVol      dense  (10);

set operation "C"         pipEnabled     true;
set operation "E"         pipEnabled     true;
set operation "I"         pipEnabled     true;
set operation "K"         pipEnabled     true;
