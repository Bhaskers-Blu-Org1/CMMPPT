//-----------------------------------------------
// PI Pegging Case 24
// Same as case 13,
// but uses an input PIP shipment sequence.
//-----------------------------------------------

release "6.0";

set problem               nPeriods          1;
set problem               multiRoute        true;

add part      "A"         material;
add operation "B";
add bomEntry  "B" "A";
add part      "C"         material;
add operation "D";
add bomEntry  "D" "C";
add part      "E"         material;
add operation "F";
add bomEntry  "F" "E";
add part      "G"         material;
add bopEntry  "B" "G";
add bopEntry  "D" "G";
add bopEntry  "F" "G";
add demand    "G" "H";
add demand    "G" "I";

set bopEntry  "B"  0      expAversion       3;
set bopEntry  "D"  0      expAversion       2;
set bopEntry  "F"  0      expAversion       1;

set part      "A"         supplyVol         dense (30);
set part      "C"         supplyVol         dense (30);
set part      "E"         supplyVol         dense (30);

set demand    "G" "H"     demandVol         dense (40);
set demand    "G" "I"     demandVol         dense (40);

set problem               pipSeqClearsNow  true;

set demand    "G" "I"     pipSeqShipPeriod 0
                          pipSeqIncShipVol 40;

set demand    "G" "H"     pipSeqShipPeriod 0
                          pipSeqIncShipVol 40;
