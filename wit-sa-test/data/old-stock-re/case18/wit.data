//-------------------------------------------------------
// Old Stock Reallocation Case 18:
// Case 16 with NSTN Build-Ahead
//-------------------------------------------------------

release "6.0";

set problem           nPeriods     2;
set problem           stockRealloc true;
set problem           perfPegging  true;

add part      "A"     capacity;
add operation "B";
add bomEntry  "B" "A";
add part      "C"     material;
add bopEntry  "B" "C";
add demand    "C" "D";
add operation "E" ;
add bomEntry  "E" "C";
add part      "F"     material;
add bopEntry  "E" "F";
add demand    "F" "G";

set part      "A"     supplyVol  dense (100  0);
set part      "C"     supplyVol  dense (  3  0);
set bomEntry  "E" 0   offset     dense ( -1 -1);
set operation "B"     incLotSize single (5);
set demand    "C" "D" demandVol  dense (  2  0);
set demand    "F" "G" demandVol  dense (  3  0);
set demand    "C" "D" priority   dense (  2  2);
set demand    "F" "G" priority   dense (  1  1);

set part      "C"     buildNstn  true;
