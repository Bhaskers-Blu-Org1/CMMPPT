//---------------------------------------------------------------
// Single Source Case 20
// Selection Splitting Variant of Case 17
//---------------------------------------------------------------

release "5.0";

set problem               nPeriods          3;
set problem               multiRoute        true;
set problem               perfPegging       true;
set problem               stockReallocation true;
set problem               selSplit          true;

add part      "A"         capacity;
add operation "B";
add bomEntry  "B" "A";
add part      "C"         capacity;
add operation "D";
add bomEntry  "D" "C";
add part      "E"         capacity;
add operation "F";
add bomEntry  "F" "E";
add part      "G"         material;
add bopEntry  "B" "G";
add bopEntry  "D" "G";
add bopEntry  "F" "G";
add operation "H";
add bomEntry  "H" "G";
add part      "I"         material;
add bopEntry  "H" "I";
add demand    "I" "J";
add operation "K";
add bomEntry  "K" "G";
add part      "L"         material;
add bopEntry  "K" "L";
add demand    "L" "M";

set operation "K"         incLotSize      single (30);

set part      "G"         singleSource    true;

set part      "A"         supplyVol       dense (  0 10 15);
set part      "C"         supplyVol       dense (  0 15 20);
set part      "E"         supplyVol       dense (  0 35 35);
set part      "G"         supplyVol       dense (100  0  0);

set bomEntry  "K"  0      offset          single (1);

set demand    "I" "J"     priority        single (1);
set demand    "L" "M"     priority        single (2);

set demand    "I" "J"     demandVol       dense (0 0 100);
set demand    "L" "M"     demandVol       dense (0 0  30);
