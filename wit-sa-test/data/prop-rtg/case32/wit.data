//-------------------------------------------------------------------
// Prop-Routing Case 32
// SelSplit with one PrAltPt switching from prov. inactive to active.
//-------------------------------------------------------------------

release "5.0";

set problem               nPeriods          1;
set problem               multiRoute        true;
set problem               penExec           true;
set problem               tieBreakPropRt    true;
set problem               selSplit          true;

add part      "A"         material;
add operation "B";
add bomEntry  "B" "A";
add part      "C"         material;
add bopEntry  "B" "C";
add operation "D";
add bomEntry  "D" "C";
add part      "E"         material;
add operation "F";
add bomEntry  "F" "E";
add part      "G"         material;
add bopEntry  "D" "G";
add bopEntry  "F" "G";
add operation "H";
add bomEntry  "H" "G";
add part      "I"         material;
add bopEntry  "H" "I";
add demand    "I" "J";

set part      "G"         propRtg           single (true);

set bopEntry  "D"  0      routingShare      single (1);
set bopEntry  "F"  0      routingShare      single (1);

set operation "B"         execPenalty       1;
set operation "D"         execPenalty       0;
set operation "F"         execPenalty       1;
set operation "H"         execPenalty       0;

set operation "H"         incLotSize        single  (40);

set part      "A"         supplyVol         dense (100);
set part      "C"         supplyVol         dense  (20);
set part      "E"         supplyVol         dense (100);

set demand    "I" "J"     demandVol         dense  (40);
