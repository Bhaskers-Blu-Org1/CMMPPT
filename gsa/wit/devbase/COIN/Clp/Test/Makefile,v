.DELETE_ON_ERROR:

.PHONY: default clean unitTest libdepend

default: unitTest

libdepend :
	(cd .. && ${MAKE} install)

unitTest: libdepend
	${MAKE} -f Makefile.test $*

clean:
	@@rm -rf Junk
	@@rm -rf $(UNAME)
	@@rm -rf dep
@


