package com.ibm.witmf;

//==============================================================================
// Class Structure
//
// Represents the structure of a model: the data independent aspects of it.
//==============================================================================

public final class Structure
{
//==============================================================================
// Public Methods
//==============================================================================

//------------------------------------------------------------------------------
// newInstance (theName)
//
// Factory method: Constructs and returns a new Structure with theName.
//------------------------------------------------------------------------------

	public static Structure newInstance (String theName)
	   {
	   return new Structure (theName);
	   }

//------------------------------------------------------------------------------
// getName ()
//
// Getter
//------------------------------------------------------------------------------

	public String getName ()
	   {
	   return myName;
	   }

//==============================================================================
// Package-Internal Constructor
//==============================================================================

   Structure (String theName)
      {
      myName = theName;
      }

//==============================================================================
// Private Fields
//==============================================================================

   private final String myName;

//==============================================================================
}
