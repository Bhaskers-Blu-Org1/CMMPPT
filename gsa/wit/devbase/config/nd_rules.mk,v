@#
# Rules used by makefiles generated by Open Interface on Unix
#
# 	@@(#)oit.makerules	3.4 93/12/03
#

# $Source: /u/implode/devbase/config/nd_rules.mk,v $
# $Revision: 1.2 $
# $Id: nd_rules.mk,v 1.2 1995/07/22 20:24:39 wally Exp $

#
# Quick roadmap:
#
# Before running make, you must set the following environment variables:
# 	ND_HOME, ND_PATH, ND_LIB
#
# The makefile should begin with:
#	include $(ND_HOME)/misc/make.defs
#
# The makefile should then define:
# 	HFILES	header files
# 	CFILES	C source files
# 	OBJS	object files
# 	LIBS	libraries
# 	EXES	executables
# 	RCOS	resource objects
#
# The makefile should end with:
#	include $(ND_HOME)/misc/make.rules
#

#----------------------------------------------------------------------------
#	Variables used by this file 
#----------------------------------------------------------------------------

#
# These definitions should be defined in make.defs. They can eventually be 
# overriden if you redefine them in the makefile between including of 
# make.defs and including of make.rules.
#
# CFLAGS	Compilation flags. Used to compile .c files into .o files
#
# LDFLAGS	Linking flags. Used to link .o and .a files into an executable
#
# RCFLAGS	Optional Flags for resource compiler.
#
# CC		Name of the C compiler
#
# RC		Name of resource compiler
#

#----------------------------------------------------------------------------
#	Suffixes and generic rules
#----------------------------------------------------------------------------

.rc.rco: $(RCOS:.rco=.rc)
	$(RC) $(RCFLAGS) $<
	@@echo '.' >>  $@@

.rc.dat: $(RCOS:.rco=.rc)
	$(RC) $(RCFLAGS) $<

@


