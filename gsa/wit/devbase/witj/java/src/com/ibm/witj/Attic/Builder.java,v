@Changed WIT-J/Java build approach.
@
text
@
//==============================================================================
// WIT-J Java Source File Builder.java.
//
// Contains the declaration of class Builder.
//==============================================================================

package       com.ibm.witj;

import static com.ibm.witj.Utility.*;

//==============================================================================
// Class Builder
//
// This class refers to all other concrete Java classes in WIT-J.
// Compiling this source file causes the source files for all of the other Java
// classes in WIT-J to be compiled if they are out-of-date.
//
// The class is not actually used by WIT-J.
//==============================================================================

final class Builder
{
//------------------------------------------------------------------------------

   private void referToAllClasses ()
      {
      CompClassID theCompClassID;

      assert (false);

      Problem   .makeInstance     ();
      Part      .makeInstance     (null, null, Part.Category.MATERIAL);
      Demand    .makeInstance     (null, null);
      Operation .makeInstance     (null, null);
      BomEntry  .makeInstance     (null, null);
      Substitute.makeInstance     (null, null);
      BopEntry  .makeInstance     (null, null);

      new ProbCoord               (null, false);
      new CompMgr                 (null);
      new Global                  (null);
      new AttRepMgr               (null);
      new StdAttRep               (null, null, null);
      new ProbCoordCI             ();
      new ProblemCI               (null);
      new GlobalCI                (null);
      new PartCI                  (null, null, true);
      new DemandCI                (null, null);
      new OperationCI             (null, null);
      new BomEntryCI              (null, null);
      new SubstituteCI            (null, null);
      new BopEntryCI              (null, null);
      new AttBldrCI               (null);
      new AttCI                   (null, null);

      theCompClassID = CompClassID.PART;
      }

//------------------------------------------------------------------------------
}
