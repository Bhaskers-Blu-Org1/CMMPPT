1.17
log
@Moved GlobalAspect responsibilities to class Problem.
Removed class GlobalAspect.
@
text
@//------------------------------------------------------------------------------
// WIT-J C++ Header File AttBldrMacs.h.
//
// Contains macros to be used by class AttBldr.
//------------------------------------------------------------------------------

#ifndef AttBldrMacs_h
#define AttBldrMacs_h

//==============================================================================
// Macro MAKE_ATT (theAttID, theCompClass, theElemTypeID, theDimID, theAccessID)
//
// Expands to code that creates a new Att according to the arguments:
//
//    theAttID:      The Att name, with the first letter in upper case,
//                   e.g. MrpSubVol.
//    theCompClass:  The Component class for the Att, e.g. Substitute.
//    theElemTypeID: Either Boolean, Integer, Double, or String.
//    theDimID:      Either Scalar or Vector.
//    theAccessID:   Either Input or Output:
//                      An Input  Att has a Set function.
//                      An Output Att does not.
//
// The code that results from an invocation of this macro is only valid within
// the scope of a non-static member function of class AttBldr.
//
// This macro is the first of four layers of macros: Specifically:
//
//    An                  invocation of   this   macro
//          expands to an invocation of a second macro,
//    which expands to an invocation of a third  macro,
//    which expands to an invocation of a fourth macro, 
//    which expands to the actual code.
//
// Consider the following example:
//
//    MAKE_ATT (MrpSubVol, Substitute, Double, Vector, Output);
//
// This expands to the following macro invocation:
//
//    MAKE_Substitute_ATT (MrpSubVol, Double, Vector, Output);
//
// This, in turn, expands to the following macro invocation:
//
//    MAKE_Double_ATT (
//       "MrpSubVol",
//       SubVectorAtt,
//       witSetSubsBomEntryMrpSubVol,
//       witGetSubsBomEntryMrpSubVol,
//       Output);
//
// This, in turn, expands to the following macro invocation:
//
//    MAKE_Output_ATT (
//       "MrpSubVol",
//       SubVectorAtt <double>,
//       witSetSubsBomEntryMrpSubVolDbl,
//       witGetSubsBomEntryMrpSubVolDbl);
//
// And this expands to the following code:
//
//    new SubVectorAtt <double> (
//       this,
//       "MrpSubVol",
//       NULL,
//       witGetSubsBomEntryMrpSubVolDbl);
//
//==============================================================================

#define MAKE_ATT(                                                              \
      theAttID,                                                                \
      theCompClass,                                                            \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theCompClass ## _ATT (                                             \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \

//==============================================================================
// Second Layer Macros
//==============================================================================

//------------------------------------------------------------------------------
// Macro MAKE_Problem_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class Problem.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Problem_ATT(                                                      \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      Problem ## theDimID ## Att,                                              \
      witSet ## theAttID,                                                      \
      witGet ## theAttID,                                                      \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_Part_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class Part.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Part_ATT(                                                         \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      Part ## theDimID ## Att,                                                 \
      witSetPart ## theAttID,                                                  \
      witGetPart ## theAttID,                                                  \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_Demand_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class Demand.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Demand_ATT(                                                       \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      Demand ## theDimID ## Att,                                               \
      witSetDemand ## theAttID,                                                \
      witGetDemand ## theAttID,                                                \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_Operation_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class Operation.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Operation_ATT(                                                    \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      Opn ## theDimID ## Att,                                                  \
      witSetOperation ## theAttID,                                             \
      witGetOperation ## theAttID,                                             \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_BomEntry_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class BomEntry.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_BomEntry_ATT(                                                     \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      BomEnt ## theDimID ## Att,                                               \
      witSetBomEntry ## theAttID,                                              \
      witGetBomEntry ## theAttID,                                              \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_Substitute_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class Substitute.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Substitute_ATT(                                                   \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      Sub ## theDimID ## Att,                                                  \
      witSetSubsBomEntry ## theAttID,                                          \
      witGetSubsBomEntry ## theAttID,                                          \
      theAccessID)                                                             \

//------------------------------------------------------------------------------
// Macro MAKE_BopEntry_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is associated with Components of class BopEntry.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_BopEntry_ATT(                                                     \
      theAttID,                                                                \
      theElemTypeID,                                                           \
      theDimID,                                                                \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theElemTypeID ## _ATT (                                            \
      #theAttID,                                                               \
      BopEnt ## theDimID ## Att,                                               \
      witSetBopEntry ## theAttID,                                              \
      witGetBopEntry ## theAttID,                                              \
      theAccessID)                                                             \

//==============================================================================
// Third Layer Macros
//==============================================================================

//------------------------------------------------------------------------------
// Macro MAKE_Boolean_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an attribute of type boolean scalar or vector.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Boolean_ATT(                                                      \
      theAttName,                                                              \
      theAttTemp,                                                              \
      theSetFuncID,                                                            \
      theGetFuncID,                                                            \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theAccessID ## _ATT (                                              \
      theAttName,                                                              \
      theAttTemp <witBoolean>,                                                 \
      theSetFuncID,                                                            \
      theGetFuncID)                                                            \

//------------------------------------------------------------------------------
// Macro MAKE_Integer_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an attribute of type integer scalar or vector.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Integer_ATT(                                                      \
      theAttName,                                                              \
      theAttTemp,                                                              \
      theSetFuncID,                                                            \
      theGetFuncID,                                                            \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theAccessID ## _ATT (                                              \
      theAttName,                                                              \
      theAttTemp <int>,                                                        \
      theSetFuncID,                                                            \
      theGetFuncID)                                                            \

//------------------------------------------------------------------------------
// Macro MAKE_Double_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an attribute of type double scalar or vector.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Double_ATT(                                                       \
      theAttName,                                                              \
      theAttTemp,                                                              \
      theSetFuncID,                                                            \
      theGetFuncID,                                                            \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theAccessID ## _ATT (                                              \
      theAttName,                                                              \
      theAttTemp <double>,                                                     \
      theSetFuncID ## Dbl,                                                     \
      theGetFuncID ## Dbl)                                                     \

//------------------------------------------------------------------------------
// Macro MAKE_String_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an attribute of type string scalar or vector.
// For more information, see macro "MAKE_ATT" above.
// Note that in this case, theAttTemp is not the name of a template, it's the
// second part of the name of an Att class.
//------------------------------------------------------------------------------

#define MAKE_String_ATT(                                                       \
      theAttName,                                                              \
      theAttTemp,                                                              \
      theSetFuncID,                                                            \
      theGetFuncID,                                                            \
      theAccessID)                                                             \
                                                                               \
   MAKE_ ## theAccessID ## _ATT (                                              \
      theAttName,                                                              \
      String ## theAttTemp,                                                    \
      theSetFuncID,                                                            \
      theGetFuncID)                                                            \

//==============================================================================
// Fourth Layer Macros
//==============================================================================

//------------------------------------------------------------------------------
// Macro MAKE_Input_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an input attribute.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Input_ATT(                                                        \
      theAttName,                                                              \
      theAttClass,                                                             \
      theSetFuncPtr,                                                           \
      theGetFuncPtr)                                                           \
                                                                               \
   new theAttClass (                                                           \
      this,                                                                    \
      theAttName,                                                              \
      theSetFuncPtr,                                                           \
      theGetFuncPtr)                                                           \

//------------------------------------------------------------------------------
// Macro MAKE_Output_ATT
//
// Expands to code that creates a new Att according to the arguments:
// The Att is an output attribute.
// For more information, see macro "MAKE_ATT" above.
//------------------------------------------------------------------------------

#define MAKE_Output_ATT(                                                       \
      theAttName,                                                              \
      theAttClass,                                                             \
      theSetFuncPtr,                                                           \
      theGetFuncPtr)                                                           \
                                                                               \
   new theAttClass (                                                           \
      this,                                                                    \
      theAttName,                                                              \
      NULL,                                                                    \
      theGetFuncPtr)                                                           \

#endif
@


