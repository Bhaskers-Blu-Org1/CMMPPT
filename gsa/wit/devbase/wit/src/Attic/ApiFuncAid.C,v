//------------------------------------------------------------------------------

void WitAfaMgr::buildFuncAid ()
   {
   WitApiFuncAid * theFuncAid;

   theFuncAid =
      new WitApiFuncAid (
         myWitRun_,
         myFuncName_,
         myReqsInitTag_,
         myReqOptStateTag_);

   myFuncName_       =                "";
   myReqsInitTag_    = WitReqsInit   ::invalid;
   myReqOptStateTag_ = WitReqOptState::invalid;

   unstoredFuncAids_.append (theFuncAid);
   }

//------------------------------------------------------------------------------

void WitAfaMgr::checkFuncAidsOrder ()
   {
   int theIdx;

   for (theIdx = 1; theIdx < myFuncAids_.length (); theIdx ++)
      nonMsgAssert (
         myFuncAids_[theIdx - 1]->myFuncName ()
         <
         myFuncAids_[theIdx    ]->myFuncName ());
   }

//------------------------------------------------------------------------------
// Implementation of class ApiFuncAid.
//------------------------------------------------------------------------------

inline WitMsgFacility & WitApiFuncAid::myMsgFac ()
   {
   return myWitRun_->myMsgFac ();
   }

//------------------------------------------------------------------------------

WitApiFuncAid::WitApiFuncAid (
         WitRun *            theWitRun,
         const WitString &   theFuncName,
         WitReqsInit::Tag    theReqsInitTag,
         WitReqOptState::Tag theReqOptStateTag):

      myWitRun_         (theWitRun),
      myFuncName_       (theFuncName),
      myReqsInitTag_    (theReqsInitTag),
      myReqOptStateTag_ (theReqOptStateTag)
   {
   if (DEBUG_MODE)
      validateAttValues ();
   }

//------------------------------------------------------------------------------

WitApiFuncAid::~WitApiFuncAid ()
   {
   }

//------------------------------------------------------------------------------

void WitApiFuncAid::startMyFunc ()
   {
   if (myReqsInitTag_ == WitReqsInit::yes)
      {
      if (myMsgFac ().mayIssue ("witFuncCalled"))
         myMsgFac () ("witFuncCalled", myFuncName_);

      if (not myWitRun_->mySession ()->active ())
         myMsgFac () ("callBeforeInitSMsg", myFuncName_);
      }

   if (myReqOptStateTag_ == WitReqOptState::active)
      if (not myWitRun_->myOptImploder ()->active ())
         myMsgFac () ("reqOptImpActiveSmsg", myFuncName_);

   if (myReqOptStateTag_ == WitReqOptState::inactive)
      if (myWitRun_->myOptImploder ()->active ())
         myMsgFac () ("reqOptImpInactiveSmsg", myFuncName_);
   }

//------------------------------------------------------------------------------

void WitApiFuncAid::validateAttValues ()
   {
   nonMsgAssert (strncmp (myFuncName_.myCstring (), "wit", 3) == 0);

   nonMsgAssert    (myReqsInitTag_    != WitReqsInit   ::invalid);
   nonMsgAssert    (myReqOptStateTag_ != WitReqOptState::invalid);

   if              (myReqOptStateTag_ == WitReqOptState::active)
      nonMsgAssert (myReqsInitTag_    == WitReqsInit   ::yes);

   if              (myReqOptStateTag_ == WitReqOptState::inactive)
      nonMsgAssert (myReqsInitTag_    == WitReqsInit   ::yes);
   }
