a3191 9

bool WitAfaMgr::compareFuncAids (
      WitApiFuncAid * theFuncAid1,
      WitApiFuncAid * theFuncAid2)
   {
   return (theFuncAid1->myFuncName () < theFuncAid2->myFuncName ());
   }

