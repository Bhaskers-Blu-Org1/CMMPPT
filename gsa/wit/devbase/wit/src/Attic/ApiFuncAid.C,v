//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Source file: "ApiFuncAid.C"
//
// Contains the implementation of the following classes:
//
//    AfaMgr.
//    ApiFuncAid.
//------------------------------------------------------------------------------

#include <AfaMgr.h>
#include <WitRun.h>
#include <OptImp.h>
#include <MsgFac.h>

//------------------------------------------------------------------------------
// Implementation of class AfaMgr.
//------------------------------------------------------------------------------

WitAfaMgr::WitAfaMgr (WitRun * theWitRun):

      myWitRun_         (theWitRun),
      unstoredFuncAids_ (),
      myFuncAids_       (),
      myFuncName_       (),
      myReqsInitTag_    (WitReqsInit::invalid),
      myReqOptStateTag_ (WitReqOptState::invalid)
   {
   setUpFuncAids ();
   }

//------------------------------------------------------------------------------

WitAfaMgr::~WitAfaMgr ()
   {
   int theIdx;

   for (theIdx = 0; theIdx < myFuncAids_.length (); theIdx ++)
      {
      delete myFuncAids_[theIdx];

      myFuncAids_[theIdx] = NULL;
      }
   }

//------------------------------------------------------------------------------

WitApiFuncAid * WitAfaMgr::findFuncAid (const char * theFuncName)
   {
   int lowerBound;
   int upperBound;
   int theIdx;
   int cmpResult;

   witAssert (strncmp (theFuncName, "wit", 3) == 0);

   lowerBound = -1;
   upperBound = myFuncAids_.length ();

   while (upperBound - lowerBound > 1)
      {
      theIdx    = (lowerBound + upperBound) / 2;

      cmpResult =
         strcmp (
            myFuncAids_[theIdx]->myFuncName ().myCstring () + 3,
            theFuncName                                     + 3);

      if (cmpResult < 0)
         lowerBound = theIdx;

      else if (cmpResult > 0)
         upperBound = theIdx;

      else
         return myFuncAids_[theIdx];
      }

   stronglyAssert (false);

   return NULL;
   }

//------------------------------------------------------------------------------

void WitAfaMgr::setUpFuncAids ()
   {
   int             theIdx;
   WitApiFuncAid * theFuncAid;

   buildAllFuncAids ();

   myFuncAids_.resize (unstoredFuncAids_.nElements ());

   theIdx = -1;

   forEachEl (theFuncAid, unstoredFuncAids_)
      {
      theIdx ++;

      myFuncAids_[theIdx] = theFuncAid;
      }

   unstoredFuncAids_.clear ();

   if (DEBUG_MODE)
      checkFuncAidsOrder ();
   }

//------------------------------------------------------------------------------

